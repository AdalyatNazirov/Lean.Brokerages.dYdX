    <Protobuf Include="Protos\cosmos\msg\v1\msg.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\cosmos\tx\signing\v1beta1\signing.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\cosmos\tx\v1beta1\service.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\cosmos\tx\v1beta1\tx.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\cosmos_proto\cosmos.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\gogoproto\gogo.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\google\api\annotations.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\google\api\http.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\tendermint\abci\types.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\tendermint\crypto\keys.proto" GrpcServices="Client" ProtoRoot="Protos\" />
