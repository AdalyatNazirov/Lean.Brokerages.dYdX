    <Protobuf Include="Protos\tendermint\types\evidence.proto" GrpcServices="Client" ProtoRoot="Protos\" />
    <Protobuf Include="Protos\tendermint\types\params.proto" GrpcServices="Client" ProtoRoot="Protos\" />
